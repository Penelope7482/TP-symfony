{# templates/home.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}Page : {{ pageTitle }} {% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>Hello, vous êtes sur la page {{ pageTitle}} !</h1>
</div>

<h1>Films</h1>
<ul>
    {% for movie in movies %}
        <li>{{ movie.title }} (Durée : {{ movie.length }} min)</li>
    {% endfor %}
</ul>


{% endblock %}